{
  "builds": [
    {
      "src": "api/agente.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/agente",
      "dest": "/api/agente"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}